{
  "name": "vue-macros",
  "version": "3.0.0-beta.8",
  "description": "Explore more macros and syntax sugar to Vue.",
  "type": "module",
  "keywords": [
    "vue-macros",
    "macros",
    "vue",
    "sfc",
    "setup",
    "script-setup",
    "macros",
    "unplugin"
  ],
  "license": "MIT",
  "homepage": "https://vue-macros.dev",
  "bugs": {
    "url": "https://github.com/vue-macros/vue-macros/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-macros/vue-macros.git",
    "directory": "packages/macros"
  },
  "author": "三咲智子 Kevin Deng <sxzz@sxzz.moe>",
  "funding": "https://github.com/sponsors/vue-macros",
  "files": [
    "*.d.ts",
    "dist",
    "volar.cjs"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./esbuild": "./dist/esbuild.js",
    "./rolldown": "./dist/rolldown.js",
    "./rollup": "./dist/rollup.js",
    "./rspack": "./dist/rspack.js",
    "./runtime": "./dist/runtime.js",
    "./vite": "./dist/vite.js",
    "./volar": {
      "types": "./volar.d.ts",
      "default": "./volar.cjs"
    },
    "./webpack": "./dist/webpack.js",
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "peerDependencies": {
    "vue": "^2.7.0 || ^3.2.25"
  },
  "dependencies": {
    "unplugin": "^2.2.2",
    "unplugin-combine": "^1.2.1",
    "@vue-macros/better-define": "3.0.0-beta.8",
    "@vue-macros/boolean-prop": "3.0.0-beta.8",
    "@vue-macros/common": "3.0.0-beta.8",
    "@vue-macros/chain-call": "3.0.0-beta.8",
    "@vue-macros/define-emit": "3.0.0-beta.8",
    "@vue-macros/config": "3.0.0-beta.8",
    "@vue-macros/define-models": "3.0.0-beta.8",
    "@vue-macros/define-prop": "3.0.0-beta.8",
    "@vue-macros/define-props": "3.0.0-beta.8",
    "@vue-macros/define-props-refs": "3.0.0-beta.8",
    "@vue-macros/define-slots": "3.0.0-beta.8",
    "@vue-macros/define-render": "3.0.0-beta.8",
    "@vue-macros/devtools": "3.0.0-beta.8",
    "@vue-macros/export-expose": "3.0.0-beta.8",
    "@vue-macros/define-stylex": "3.0.0-beta.8",
    "@vue-macros/export-props": "3.0.0-beta.8",
    "@vue-macros/hoist-static": "3.0.0-beta.8",
    "@vue-macros/export-render": "3.0.0-beta.8",
    "@vue-macros/jsx-directive": "3.0.0-beta.8",
    "@vue-macros/named-template": "3.0.0-beta.8",
    "@vue-macros/script-lang": "3.0.0-beta.8",
    "@vue-macros/setup-block": "3.0.0-beta.8",
    "@vue-macros/reactivity-transform": "3.0.0-beta.8",
    "@vue-macros/setup-component": "3.0.0-beta.8",
    "@vue-macros/setup-sfc": "3.0.0-beta.8",
    "@vue-macros/short-bind": "3.0.0-beta.8",
    "@vue-macros/short-vmodel": "3.0.0-beta.8",
    "unplugin-vue-define-options": "3.0.0-beta.8",
    "@vue-macros/volar": "3.0.0-beta.8",
    "@vue-macros/short-emits": "3.0.0-beta.8"
  },
  "devDependencies": {
    "vue": "^3.5.13"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "DEV=true tsup"
  }
}